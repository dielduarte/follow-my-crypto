{
  "name": "followmycrypto",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "ios": "react-native run-ios --simulator",
    "postinstall": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
